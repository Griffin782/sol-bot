{
  "name": "sol-bot-sniper",
  "version": "5.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "analyze": "ts-node src/enhanced/analyze-trading-session.ts",
    "monitor": "ts-node src/live-monitor.ts",
    "test-email": "node send_test_email.js",
    "check-config": "node scripts/check-config-imports.js",
    "dev:test5": "ts-node src/index.ts --max-trades 5",
    "dev:test20": "ts-node src/index.ts --max-trades 20",
    "dev:test50": "ts-node src/index.ts --max-trades 50",
    "dev:safe": "ts-node src/index.ts --max-trades 5 --max-loss 0.01",
    "prebuild": "echo 'Config check disabled for live trading'",
    "predev": "echo 'Config check disabled for live trading'",
    "config": "ts-node src/core/CONFIG-WIZARD.ts",
    "validate": "ts-node src/core/SMART-CONFIG-VALIDATOR.ts",
    "preflight": "ts-node src/core/PRE-FLIGHT-CHECK.ts",
    "smart-setup": "ts-node src/core/SMART-CONFIG-SYSTEM.ts",
    "test-smart-config": "ts-node src/core/test-smart-config.ts",
    "csv-monitor": "ts-node src/monitoring/csv-monitor.ts",
    "cleanup": "ts-node src/utils/process-cleanup.ts",
    "cleanup:status": "ts-node src/utils/process-cleanup.ts status",
    "cleanup:dry-run": "ts-node src/utils/process-cleanup.ts dry-run",
    "mi-baseline": "ts-node src/market-intelligence/standalone-recorder.ts",
    "mi-compare": "ts-node src/market-intelligence/reports/compare-bot-to-market.ts",
    "mi-analysis": "ts-node src/market-intelligence/reports/daily-analysis.ts",
    "mi-test": "ts-node src/market-intelligence/test-setup.ts",
    "check-db": "ts-node src/market-intelligence/check-database-status.ts",
    "post-check": "ts-node scripts/post-session-check.ts",
    "verify-fixes": "ts-node scripts/verify-critical-fixes.ts"
  },
  "keywords": [
    "solana",
    "trading",
    "bot",
    "sniper"
  ],
  "author": "sol-bot",
  "license": "ISC",
  "description": "Enhanced Solana Token Sniper with 5x+ Detection",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/blessed": "^0.1.25",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.1",
    "@types/ws": "^8.5.13",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@jup-ag/api": "^6.0.44",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@project-serum/anchor": "^0.26.0",
    "@pump-fun/pump-swap-sdk": "^1.9.0",
    "@solana/addresses": "^5.0.0",
    "@solana/rpc-types": "^5.0.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "@triton-one/yellowstone-grpc": "^4.0.2",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.4.1",
    "blessed": "^0.1.81",
    "bs58": "^6.0.0",
    "chalk": "^5.4.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "luxon": "^3.5.0",
    "nodemailer": "^7.0.5",
    "papaparse": "^5.5.3",
    "protobufjs": "^7.5.4",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.27.8",
    "ws": "^8.18.0"
  }
}
